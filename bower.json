  "homepage": "https://github.com/justinsa/angular-local-storage-service",
  "main": "./angular-local-storage-service.js",
  "ignore": [
    ".jshintrc",
    "gulpfile.js",
    "package.json",
    "test.html",
    "tests.js"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/justinsa/angular-local-storage-service.git"
  },
  "dependencies": {
    "angular": ">=1.2.19",
    "angular-cookies": ">=1.2.19"
  },
  "devDependencies": {
    "angular-mocks": "*",
    "mocha": "*",
    "should": "*",
    "sinon": "*"
  }
}
